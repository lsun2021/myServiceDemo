<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.demo.myservicedemo">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".demo.Main3Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name=".MyService"
            android:enabled="true"
            android:exported="true" />
        <service
            android:name=".bindservice.MyBindService"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".MainActivity" />

        <service android:name=".bindservice.MyIntentService" />
        <activity android:name=".bindservice.Main2Activity"/>

        <!--<activity android:name=".demo.Main3Activity" />-->

        <service
            android:name=".demo.DownloadService"
            android:enabled="true"
            android:exported="true"/>
    </application>

</manifest>